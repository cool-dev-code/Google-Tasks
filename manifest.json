{
	"manifest_version":2,
	
	"name":"Google Tasks",
	"description":"Unofficial Google Tasks Fullscreen.",
	"version":"1.0.0",
	"icons":
		{
			"128":"img/icon.png"
		},
	"app":
		{
			"background":
				{
					"scripts":["js/background.js"],
					"pages":["html/embed.html"]
				}
		},
	"permissions":
		[
			"webview",
			"fullscreen",
			"notifications"
		]
}